    <!-->  

        Visualisation Tool & Windows. 

    <!-->

    <!-->  
    Gammatone Filtered Audio.
    <!-->
    <module>
        <name> frequencyVisualisation </name>
        <parameters> --name /gammatoneFilteredAudioVisualiser </parameters>
        <node> icub </node>
    </module>

    <module>
        <name> yarpview </name>
        <parameters> --x 100 --y 100 --h 320 --w 240 --r 33 --name /gammatoneFilteredAudioView </parameters>
        <node> icub </node>
    </module>


